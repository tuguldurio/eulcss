{
  "name": "eulercss",
  "title": "EulerCSS",
  "version": "0.3.3",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"vite\" \"npx tailwindcss -i ./src/assets/tailwind.css -o ./public/tailwind_build.css --watch\"",
    "build": "npx tailwindcss -i ./src/assets/tailwind.css -o ./public/tailwind_build.css && vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "@headlessui/vue": "^1.4.2",
    "@vueuse/core": "^7.2.2",
    "concurrently": "^7.0.0",
    "highlight.js": "^11.3.1",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.2.24",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.4",
    "tailwindcss": "^3.0.6",
    "typescript": "^4.5.3",
    "unplugin-vue-components": "^0.17.6",
    "vite": "^2.7.1",
    "vue-tsc": "^0.29.8"
  }
}
